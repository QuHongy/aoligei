
@{ ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var a = ViewBag.hospital as HXWeb.Models.Hospital;
    }

<link href="~/Content/Site.css" rel="stylesheet" />
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<link href="~/Content/global.css" rel="stylesheet" />

<h2>接种日期</h2>
<h4>@ViewBag.hospital.HospitalName</h4>
<br />
<form method="post">
    <input type="text" class="hidden" name="ID" value="@ViewBag.ID" />
    <div class="form-group">
        <label class="control-label col-md-2">起始时间</label>
        <div class="col-md-10">
            <input type="date" class="form-control text-box  single-line" name="StartDate" value=@a.StartDate />
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">结束时间</label>
        <div class="col-md-10">
            <input type="date" class="form-control text-box  single-line" name="EndDate" value=@a.EndDate />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
           <input type="submit" class="btn btn-primary"/>
        </div>
    </div>
</form>




